  "keywords": [
    "next",
    "nextjs",
    "next.js",
    "less",
    "less plugin",
    "antd",
    "ant design",
    "antd plugin",
    "plugin",
    "css module",
    "next plugin"
  ],
  "devDependencies": {},
  "dependencies": {
    "clone": "^2.1.2",
    "less-loader": "^7.1.0",
    "less-vars-to-js": "^1.3.0",
    "null-loader": "^4.0.1"
  }
}
